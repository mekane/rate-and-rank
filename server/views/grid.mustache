{{> pageStart }}
<link rel="stylesheet" href="/css/grid.css">
<!-- App View for Data Grid {{gridName}} -->
<div id="grid"></div>
<script type="text/javascript" src="/serverBackedGridView.bundle.js"></script>
<script type="text/javascript">
    window.initGlobalUndos = {{ numberOfUndos }};
    window.initGlobalRedos = {{ numberOfRedos }};

    DataGrid.initGrid('#grid', '{{& actionUrl }}', '{{& getStateUrl }}');
</script>
{{> pageEnd }}